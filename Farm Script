local Button = Tab:CreateButton({
	Name = "Fast Punch",
	Callback = function()
		old = hookfunction(wait,function(t)
			if not checkcaller() and getcallingscript().Name == "MouseIcon" then
				return old()
			end
			return old(t)
		end)
	end,
})
